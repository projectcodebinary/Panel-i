<div class="col-sm-2" id="list">
		
		<h3><a class="btn btn-success" id="new" href="/ir">New</a></h3>
		<h3><a class="btn btn-warning" id="history" href="/ir/list">History</a></h3>
	</div>

<div class="col-sm-10" id="table">
<div class="table-responsive">
<table class="table table-striped table-bordered">
    <thead >
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Company</th>
            <th>Position</th>
            <th>Q1</th>
            <th>Q2</th>
            <th>Q3</th>
            <th>Q4</th>
            <th>Q5</th>
            <th>Q6</th>
            <th>Q7</th>
            <th>Q8</th>
            <th>Q9</th>
            <th>Q10</th>
            <th>Q11</th>
            <th>Q12</th>
            <th>Q13</th>
            <th>Q14</th>
            <th>Q15</th>
            <th>Additional Remarks</th>
            <th>Convert</th>
        </tr>
    </thead>
    <tbody>
        {{#each list }}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.companyname}}</td>
            <td>{{this.position}}</td>
            <td>{{this.question1}}</td>
            <td>{{this.question2}}</td>
            <td>{{this.question3}}</td>
            <td>{{this.question4}}</td>
            <td>{{this.question5}}</td>
            <td>{{this.question6}}</td>
            <td>{{this.question7}}</td>
            <td>{{this.question8}}</td>
            <td>{{this.question9}}</td>
            <td>{{this.question10}}</td>
            <td>{{this.question11}}</td>
            <td>{{this.question12}}</td>
            <td>{{this.question13}}</td>
            <td>{{this.question14}}</td>
            <td>{{this.question15}}</td>
            <td>{{this.remarks}}</td>
            <td>{{this.convert}}</td>            
        </tr>
        {{/each}}
    </tbody>
</table>
<h3><a class="btn btn-primary" id="history" href="/ir">Back</a></h3>
</div>

</div>


