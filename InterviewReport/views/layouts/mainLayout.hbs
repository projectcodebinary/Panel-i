<!DOCTYPE html>
<html lang="en">
<head>
  <title>Interview Report</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
  <style>
      
    #left{   
    position: relative;   
    }
#new{
    border-radius: 20px;
    width:150px;
    margin:60px 50px 0px 30px ;
} 
#history{
    border-radius: 20px;
    width:150px;
    margin:30px 50px 0px 30px ;
}
#right{
    position: absolute;  
    right:0px;
    border-left: 1px solid rgb(53, 52, 52);
}
 #list{   
    position: relative; 
    height: 625px;
    border-right: 1px solid rgb(53, 52, 52);  
    }
#table{
    position: absolute;  
    right:0px;
    top:20px;
}
.panel{
    margin: 10px;
}

#submit{
    border-radius: 20px;
    width:180px;			
}

@media screen and (max-width:1365px){
    .container-fuild{
        width: 100%;
    }
    #left, #list{
        width: 20%;
    }
    #right, #table{
        width: 80%;
    }
}
@media screen and (max-width:1024px){
    .container-fuild{
        width: 100%;
    }
    #left, #list{
        width: 22%;
    }
    #right, #table{
        width: 78%;
    }
}


@media screen and (max-width:910px) and (min-width:768px){
    #left, #list{
        width: 25%;
    }
    #right, #table{
        width: 75%;
    }

   #new{
       width: 140px;
   }
   #history{
       width:140px;
   }
}


@media screen and (max-width:767px) {
   #left{
        width: 100%;
        
    }
    #right{
        width: 100%;      
        border-left: 0px;
        border-top: 1px solid rgb(53, 52, 52);
    }
    #list{
        width: 100%;
        height: 0%;
        border-right: 0px;
        border-bottom: 1px solid rgb(53, 52, 52);
    }
   #table{
        width: 100%;      
        top:160px;      
   }
    #new{
        border-radius: 20px;
        width:150px;
        margin:20px 0px 0px 20px ;
        
    } 
    #history{
        border-radius: 20px;
        width:150px;
        margin:0px 0px 0px 20px ;       
    }
    
 
    
}

@media screen and (max-width:468px) {
   
    #left{
        width: 100%;
        
    }
    #right{
        width: 100%;      
        border-left: 0px;
        border-top: 1px solid rgb(53, 52, 52);
    }
    #list{
        width: 100%;
        height: 0%;
        border-right: 0px;
        border-bottom: 1px solid rgb(53, 52, 52);
    }
   #table{
        width: 100%;      
        top:160px;      
   }
    #new{
        border-radius: 20px;
        width:150px;
        margin:20px 0px 0px 20px ;
        
    } 
    #history{
        border-radius: 20px;
        width:150px;
        margin:0px 0px 0px 20px ;       
    }
    
    #submit{
    border-radius: 20px;
    width:100%;			
    }
}

 </style>
  
</head>
